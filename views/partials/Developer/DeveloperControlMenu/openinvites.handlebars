<div class="table-wrapper open-invites">
    <h2>Open Invites</h2>
    <table>
        <thead>
            <tr>
                <th>Project id#</th>
                <th>Host: Description</th>
                <th>Project Due Date</th>
                <th>Project Details</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            {{#each project}}
            <tr>
                <td>{{project.id}}</td>
                <td>{{project.description}}</td>
                <td>{{project.duedate}}</td>
                <td><a href="" class="button alt small fit icon fa-info-circle">View Project</a></td>
                <td><a href="" class="button fit small icon fa-check">Accept Request</a></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>